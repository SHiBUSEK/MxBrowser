{
  "name": "mxbrowser",
  "version": "0.2.2",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "Shibusek",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "com.shibusek.mxbrowser",
    "productName": "MxBrowser",
    "directories": {
      "output": "dist"
    },
    "files": [
      "assets/",
      "main.js",
      "preload.js",
      "index.html",
      "package.json",
      "styles.css",
      "tabs.js",
      "iframeStyler.js",
      "newtab.html",
      "styles2.css"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "SHiBUSEK",
        "repo": "MxBrowser"
      }
    ]
  },
  "dependencies": {
    "electron-log": "^5.2.2",
    "electron-updater": "^6.3.9"
  }
}
